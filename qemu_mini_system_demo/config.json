{
    "product_name": "qemu_mini_system_demo",
    "ohos_version": "OpenHarmony 1.0",
    "device_company": "qemu",
    "board": "arm_mps2_an386",
    "kernel_type": "liteos_m",
    "kernel_version": "3.0.0",
    "subsystems": [
      {
        "subsystem": "kernel",
        "components": [
          { "component": "liteos_m", "features":[ "enable_ohos_kernel_liteos_m_test = true" ] }
        ]
      },
      {
        "subsystem": "hiviewdfx",
        "components": [
          { "component": "hievent_lite", "features":[] }
        ]
      },
      {
        "subsystem": "distributed_schedule",
        "components": [
          { "component": "samgr_lite", "features":[] }
        ]
      },
      {
        "subsystem": "utils",
        "components": [
          { "component": "kv_store", "features":[] },
          { "component": "os_dump", "features":[] }
        ]
      },
      {
        "subsystem": "startup",
        "components": [
          { "component": "bootstrap_lite", "features":[] },
          { "component": "syspara_lite", "features":[] }
        ]
      },
      {
        "subsystem": "security",
        "components": [
          { "component": "huks", "features":
            [
              "huks_use_lite_storage = true",
              "huks_use_hardware_root_key = true",
              "huks_config_file = \"hks_config_lite.h\""
            ]
          }
        ]
      }
    ],
    "third_party_dir": "",
    "product_adapter_dir": "//vendor/ohemu/qemu_mini_system_demo/hals"
  }
